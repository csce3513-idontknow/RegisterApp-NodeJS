<html>
    <head>
        <title>Product Sales</title> 
        <meta name="viewport" content="width=device-width, initial-scale=1"> 

        <link rel="stylesheet" type="text/css" href="/styles/master.css">
        <link rel="stylesheet" type="text/css" href="/styles/productSales.css">
        
        <script type="text/javascript" src="/scripts/master.js" defer></script>
    </head>

    <body>
        <div class="header">
            <h1 class="primary-message">Products Sales</h1>
        </div>

        <div id="main" class="main">
            <div id="error" <% if (!locals.errorMessage || (locals.errorMessage === "")) { %> class="hidden" <% } %>>
                <h4 id="errorMessage" class="error-message">
                    <% if (locals.errorMessage && (locals.errorMessage !== "")) { %>
                        <%= locals.errorMessage %>
                    <% } %>
                </h4>
            </div>
            <div class="mainMenuActionsWrapper">
                <div>
                    <h2>Products Sold</h2>
                    <table id="productsSold">
                        <tr>
                            <th>Lookup Code</th>
                            <th>Price of item</th>
                            <th>Units Sold</th>
                            <th>Sale sum of product</th>
                        </tr>
                    </table>
                </div>
            </div>
        </div>

        <div class="footer">
            <img id="signOutImage" src="/images/Sign-Out.png" alt="Sign Out" class="footer" />
        </div>
    </body> 
</html>